{
  "name": "grocerylist",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "start": "concurrently \"npm run rn:start\" \"npm run server\" \"npm run adb:reverse\"",
    "rn:start": "react-native start",
    "start:clean": "concurrently \"watchman watch-del-all\" \"react-native start --reset-cache\"  \"npm run server\" \"npm run adb:reverse\"",
    "tsc:all": "npx tsc --project tsconfig.json",
    "prettier:all": "prettier --write ./src && prettier --write App.tsx",
    "test": "jest",
    "server": "json-server ./server/db.json",
    "adb:reverse": "adb reverse tcp:3000 tcp:3000"
  },
  "dependencies": {
    "@gluestack-style/react": "latest",
    "@gluestack-ui/config": "latest",
    "@gluestack-ui/themed": "latest",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@tanstack/react-query": "^5.35.5",
    "axios": "^1.6.8",
    "i18next": "^23.11.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^14.1.1",
    "react-native": "0.72.3",
    "react-native-safe-area-context": "^4.10.1",
    "react-native-screens": "^3.31.1",
    "react-native-svg": "13.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.23.9",
    "@babel/preset-env": "7.23.9",
    "@babel/runtime": "7.23.9",
    "@react-native-community/eslint-config": "3.2.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/metro-config": "0.73.5",
    "@tanstack/eslint-plugin-query": "^5.32.1",
    "@testing-library/react-native": "12.4.3",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jest": "29.5.12",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "@types/react-test-renderer": "18.0.7",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.0.0",
    "babel-jest": "29.6.3",
    "babel-loader": "8.3.0",
    "babel-plugin-module-resolver": "5.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "27.6.3",
    "jest": "29.6.3",
    "json-server": "^1.0.0-alpha.23",
    "prettier": "2.8.8",
    "react-dom": "18.2.0",
    "react-native-svg-transformer": "^1.3.0",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=16"
  }
}
